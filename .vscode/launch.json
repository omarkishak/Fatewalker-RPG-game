{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run FateWalker",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/FateWalker.exe",
      "cwd": "${workspaceFolder}",
      "console": "externalTerminal",
      "stopAtEntry": false
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "e:/College/Projects/FateWalker",
      "program": "e:/College/Projects/FateWalker/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}